@model IEnumerable<web_banvemaybay.Models.HangHK>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/Layout_Admin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenHang)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hinhanh)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenHang)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hinhanh)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDhang }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDhang }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDhang })
        </td>
    </tr>
}

</table>
