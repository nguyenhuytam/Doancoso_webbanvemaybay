<body>
    <!------------------------------- <header> ------------------------------->
    <div id="header">
        <ul id="nav">     
            <li><a href="@Url.Action("Clear", "Home")"  id="clearLink">Airplane<span> Ticket</span></a></li>
            <li><a href="@Url.Action("Clear", "Home")"  id="clearLink">Đặt vé máy bay</a></li>
            <li><a href="@Url.Action("GioiThieu", "Home")">Giới thiệu</a></li>
            <li><a href="@Url.Action("LienHe", "Home")">Liên hệ</a></li>
            <li><a href="@Url.Action("SendOTP", "Check")">Tra vé</a></li>
            @functions {

                // Hàm kiểm tra quyền truy cập
                public bool CheckPermission(string IDchucvu)
                {
                    if (IDchucvu == "1")
                    {
                        return true;
                    }
                    else if (IDchucvu == "2" || IDchucvu == null)
                    {
                        return false;
                    }
                    return false;
                }
            }
            @if (Session["IDtaikhoan"] != null)
            {
                web_banvemaybay.Models.web_banvemaybayEntities db = new web_banvemaybay.Models.web_banvemaybayEntities();
                if (CheckPermission(Session["IDchucvu"]?.ToString()))
                {
                    <li>
                        <a href="#">
                            <p>@Session["HovaTen"]</p>
                            <i class="nav-arrow-down ti-angle-down"></i>
                        </a>
                        <ul class="subnav">
                            <li><a href="/Login/Logout">Đăng xuất</a> </li>
                            <li><a href="/Admin/Chuyenbays/Index">Quản Lí</a></li>
                        </ul>
                    </li>
                }
                else
                {
                    <li>
                        <a href="#">
                            <p>@Session["HovaTen"]</p>
                            <i class="nav-arrow-down ti-angle-down"></i>
                        </a>
                        <ul class="subnav">
                            <li><a href="/Login/Logout">Đăng xuất</a> </li>
                            <li><a href="@Url.Action("LichSuVe", "Check")">Lịch sử vé</a></li>
                        </ul>
                    </li>
                }
            }
            else
            {
                <li>
                    <a href="#">
                        Tài khoản
                        <i class="nav-arrow-down ti-angle-down"></i>
                    </a>
                    <ul class="subnav">
                        <li><a href="/Login/Login">Đăng nhập</a> </li>
                        <li><a href="/Login/Register">Đăng ký</a></li>
                    </ul>
                </li>
            }
        </ul>
        <div id="mobile-menu" class="mobile-menu-btn">
            <i class="fa-solid fa-bars"></i>
        </div>
    </div>

    @RenderBody()
